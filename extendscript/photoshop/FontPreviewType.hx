package extendscript.photoshop;

/* The type size to use for font previews in the Type tool font menus. */
@:native("FontPreviewType") extern enum FontPreviewType {
	
	/* No preview. */
	NONE; 

	/* Small type. */
	SMALL; 

	/* Medium type. */
	MEDIUM; 

	/* Large type. */
	LARGE; 

	/* Extra large preview. */
	EXTRALARGE; 

	/* Huge preview. */
	HUGE; 


}