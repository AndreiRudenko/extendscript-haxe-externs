package extendscript.photoshop;

/* The measurement unit for ruler increments. */
@:native("Units") extern enum Units {
	
	/* Pixels. */
	PIXELS; 

	/* Inches. */
	INCHES; 

	/* Centimeters. */
	CM; 

	/* Millimeters. */
	MM; 

	/* Points. */
	POINTS; 

	/* Picas. */
	PICAS; 

	/* Percent. */
	PERCENT; 


}