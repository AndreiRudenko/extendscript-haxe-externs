package extendscript.photoshop;

/* Geometric options for shapes, such as the iris shape in the Lens Blur Filter. */
@:native("Geometry") extern enum Geometry {
	
	/* Triangle. */
	TRIANGLE; 

	/* Pentagon. */
	PENTAGON; 

	/* Hexagon. */
	HEXAGON; 

	/* Square. */
	SQUARE; 

	/* Heptagon. */
	HEPTAGON; 

	/* Octagon. */
	OCTAGON; 


}